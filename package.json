{
  "name": "tashkent-voxel-vision",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "pnpm -C apps/web dev",
    "build": "pnpm -r build",
    "data:manifest:validate": "node packages/data/scripts/manifest-validate.mjs",
    "data:release:init": "node packages/data/scripts/release-init.mjs",
    "data:aoi:write": "node packages/data/scripts/aoi-write.mjs",
    "data:grid": "node packages/data/scripts/grid.mjs",
    "data:osm:fetch": "node packages/data/scripts/osm-fetch.mjs",
    "data:osm:extract": "node packages/data/scripts/osm-extract.mjs",
    "data:buildings:heights": "node packages/data/scripts/buildings-heights.mjs",
    "data:buildings:lod": "node packages/data/scripts/buildings-lod.mjs",
    "data:metrics:grid": "node packages/data/scripts/metrics-grid.mjs",
    "data:tiles:buildings": "node packages/data/scripts/tiles-buildings.mjs",
    "data:tiles:base": "node packages/data/scripts/tiles-base.mjs",
    "data:green:build": "node packages/data/scripts/green-build.mjs",
    "data:serve": "node packages/data/scripts/serve-data.mjs",
    "data:smoke": "node --test packages/data/test/aoi-grid-smoke.test.mjs",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "format": "prettier -w ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.7.3"
  }
}
