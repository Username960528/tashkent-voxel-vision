{
  "name": "tashkent-voxel-vision",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "pnpm -C apps/web dev",
    "build": "pnpm -r build",
    "data:manifest:validate": "node packages/data/scripts/manifest-validate.mjs",
    "data:release:init": "node packages/data/scripts/release-init.mjs",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "format": "prettier -w ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.7.3"
  }
}
